    <link rel="stylesheet" type="text/css" href="style.css"/>
    <link rel="icon" href="/image/main/ask.png" type="image/x-icon"/>
    <style>
        .header {
            position: fixed;
            top: 0%;
            left: 0px;
            right: 0px;
            display: block;
            width: 100%;
            height: 55px;
            max-width: 415px;
            min-width: 320px;
            border-bottom: 2px solid #000;
            background-color: #bb070e;
        }
        .nav {
            position: fixed;
            left: 0px;
            right: 0px;
            bottom: -1%;
            display: inline-block;
            width: 100%;
            height: 55px;
            max-width: 415px;
            min-width: 320px;
            border-top: 2px solid #000;
            border-bottom: 2px solid #000;
            background-color: #ffc200;
        }
        .main {
            
            
        }
        .ask {
            position: fixed;
            left: 0px;
            right: 0px;
            bottom: 7px;
            z-index: 1;
            display: block;
            width: 81px;
            height: 81px;
            margin-right: auto;
            margin-left: auto;
        }
        .map {
            position: fixed;
            left: 16%;
            bottom: 8px;
            display: block;
            overflow: visible;
            width: 34px;
            float: none;
        }
        .profile {
            position: fixed;
            right: 16%;
            bottom: 8px;
            display: block;
            width: 34px;
            margin-right: auto;
            padding-left: 0px;
        }
        .search {
            position: fixed;
            left: 12px;
            top: 8px;
            width: 65%;
            height: 30px;
            padding: 8px 13px 0px 41px;
            border-style: none none solid;
            border-width: 1px;
            border-color: #000 #000 hsla(0, 0%, 100%, .74);
            background-color: transparent;
            color: white;
            font-size: 17px;
            font-style: normal;
            
        }
  
